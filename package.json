{
  "name": "@rr0/udb",
  "version": "1.0.3",
  "description": "UFO database reader",
  "main": "udb.js",
  "scripts": {
    "clean": "rm -Rf node_modules && rm package-lock.json",
    "prebuild": "npm install && rm -Rf build",
    "build": "tsc",
    "build:web": "rm -Rf build && tsc --project tsconfig-web.json",
    "test": "jest",
    "cli": "ts-node $NODE_DEBUG_OPTION -- src/udb.ts",
    "cli:dev": "ts-node-dev $NODE_DEBUG_OPTION --trace-warnings --trace-deprecation -- src/udb.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RR0/uDb.git"
  },
  "keywords": [
    "ufo",
    "database",
    "larry",
    "hatch",
    "csv",
    "xml"
  ],
  "author": "Jérôme Beau (RR0)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RR0/uDb/issues"
  },
  "homepage": "https://github.com/RR0/uDb#readme",
  "dependencies": {
    "bops": "^1.0.0",
    "build": "^0.1.4",
    "commander": "^2.9.0",
    "json2xml": "^0.1.3",
    "rm": "^0.1.8"
  },
  "devDependencies": {
    "@types/jest": "~29.1.2",
    "@types/node": "~18.11.0",
    "@types/systemjs": "~6.1.1",
    "jest": "~29.2.0",
    "jest-junit": "~14.0.1",
    "source-map-support": "~0.5.21",
    "ts-jest": "~29.0.3",
    "ts-node": "~10.9.1",
    "ts-node-dev": "~2.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.8.4",
    "typings": "~2.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
